# top-most EditorConfig file
root = true

# CMake settings
[CMakeLists.txt]
indent_style = space
indent_size = 4

# Source code settings
[*.cpp]
indent_style = space
indent_size = 4

[*.h]
indent_style = space
indent_size = 4

# Default settings
[*]
indent_style = space
indent_size = 4
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true
